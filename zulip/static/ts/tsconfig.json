{
  "compilerOptions": {
    "outDir": "../webpack-bundles/",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "suppressImplicitAnyIndexErrors": true,
    "newLine": "lf",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": false
  },
  "rootDir": "../.."
}
